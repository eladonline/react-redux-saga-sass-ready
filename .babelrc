{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "src": "./src",
        "themContainers": "./containers",
        "themComponents": "./components",
        "components": "./src/components"
      }
    }],
    ["import", {
      "libraryName": "antd"
    }],
    [
      "inline-import",
      {
        "extensions": [".css", ".scss", ".png", ".jpg"]
      }
    ],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
  ]
}
